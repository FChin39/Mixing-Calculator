<template name="TPL_picker-view">
  <picker-view <% for (let i = 0; i< props.length; i++) { %>
    <%=props[i]%>="{{node.props['<%=props[i]%>']}}" <% } %>
  >
    <block a:for="{{node.children}}" key="{{item.id}}">
      <picker-view-column>
        <view a:for="{{item.children}}">
          <template is="TPL_TRAVERSAL" data="{{root: item}}" />
        </view>
      </picker-view-column>
    </block>
  </picker-view>
</template>